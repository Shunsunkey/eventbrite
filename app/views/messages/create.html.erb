<%= form_with(model: @message, url: messages_path, method: :post) do |form| %>
    <div>
      <%= form.label :subject %>
      <%= form.text_field :subject %>
    </div>
    <div>
      <%= form.label :body %>
      <%= form.text_area :body %>
    </div>
    <div>
      <%= form.submit %>
    </div>
  <% end %>
  